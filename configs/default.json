{
  "production": {
    "envName": "production",
    "protocol":"http",
    "host":"localhost",
    "databases": {
      "db_migration":{
        "host"     : "192.*.*.*",
        "user"     : "dev-*-*",
        "password" : "pwd",
        "dialect"  : "postgres",
        "connectionURI": "postgres://dev-*-*:pwd@192.*.*.*:5432/db_migration",
        "pool": {
          "max": 5,
          "min": 0,
          "acquire": 30000,
          "idle": 10000,
          "evict": 60000,
          "handleDisconnects": true,
          "timezone":"utc"
        }
      }
    },
    "liquibase_connection_uri":"jdbc:postgresql://192.168.2.213:5432/",
    "session":{
      "cookie":{"secure":false,"maxAge":3600000}
    },
    "elasticsearch":{
      "ELASTIC_URL":"",
      "ELASTIC_REQ_TIMEOUT":600000
    },
    "logger":{
      "LOG_LABEL":"prod-boilerplate"
    },
    "swagger":{
      "enable":false
    }
  },
