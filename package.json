  "license": "ISC",
  "dependencies": {
    "async-foreach": "0.1.3",
    "axios": "^0.19.0",
    "body-parser": "1.19.0",
    "elasticsearch": "^16.3.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "method-override": "3.0.0",
    "moment": "2.24.0",
    "nodemailer": "6.3.0",
    "q": "1.5.1",
    "stryker-cli": "^1.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.1.0",
    "winston-elasticsearch": "^0.7.13"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "@stryker-mutator/core": "^1.1.1",
    "@stryker-mutator/javascript-mutator": "^1.1.1",
    "@stryker-mutator/jest-runner": "^1.1.1"
